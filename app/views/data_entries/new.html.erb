<h1>Data Entry Form</h1>

<%= form_with(model: @data_entry, url: data_entries_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :value %>
    <%= form.text_field :value %>
  </div>

  <div class="actions">
    <%= form.submit "Submit" %>
  </div>
<% end %>
