<h1>Data Entries</h1>

<%= link_to 'New Data Entry', new_data_entry_path, class: 'btn btn-primary' %>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @data_entries.each do |data_entry| %>
      <tr>
        <td><%= data_entry.name %></td>
        <td><%= data_entry.value %></td>
      </tr>
    <% end %>
  </tbody>
</table>
